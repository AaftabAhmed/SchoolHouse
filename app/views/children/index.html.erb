<h1>Profile <%= current_user.email.split('@')[0]%> </h1>

<% @children.each do |child| %>
  <h2> <%= link_to child.name, child %> </h2>
  <h3> Age : <%= child.age %> </h3>
  <h3> Grade : <%= child.grade %> </h3>
  <hr/>
<% end %>

<%= link_to "Back", root_path %>
<%= link_to "Add Child", new_child_path %>
